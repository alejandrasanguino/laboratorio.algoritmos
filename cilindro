#include "stdafx.h"
#include <iostream>
#include <conio.h>
#include <math.h>
using namespace std;
float vol_cilindro(float radio, float altura);

void main () 
{
	float radio, altura, volumen;
	cout<<"ingrese el radio:" ;
	cin>>radio;
	cout<<"ingrese el altura:" ;
	cin>>altura;
	volumen= vol_cilindro(radio,altura);
	cout<<"el valor es:" <<volumen;
	getch();
}
float vol_cilindro (float r, float a)
{
	float v,pi;
	pi=3.141516;
		v=pi*r*r*a;
		return v;
}
